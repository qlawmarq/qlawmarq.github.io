{
  "name": "svelte-portfolio",
  "description": "An example of a portfolio site created with Svelte",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://qlawmarq.github.io",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/qlawmarq/qlawmarq.github.io.git"
  },
  "author": "Masaki Yoshiiwa <masaki.yoshiiwa@gmail> (https://github.com/qlawmarq/)",
  "engines": {
    "node": "v20"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "e2e:test": "playwright test",
    "e2e:update": "playwright test -u",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin prettier-plugin-svelte --check . && eslint .",
    "format": "prettier --plugin prettier-plugin-svelte --write .",
    "typesafe-i18n": "typesafe-i18n"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.25.1",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.34.0",
    "isbot": "^3.6.13",
    "ky": "^1.4.0",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.4",
    "tslib": "^2.6.2",
    "typesafe-i18n": "^5.26.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.3"
  },
  "type": "module"
}
